#include "holberton.h"

/**
 * times_table - prints the 9 times table.
 *
 * Return: void.
 */

int times_table(int n)
{
	int tim0 = n * 0;
	int tim1 = n * 1;
	int tim2 = n * 2;
	int tim3 = n * 3;
	int tim4 = n * 4;
	int tim5 = n * 5;
	int tim6 = n * 6;
	int tim7 = n * 7;
	int tim8 = n * 8;
	int tim9 = n * 9; 

	for (n = 0; n <= 9; n++)

	{
		_putchar(tim1);
	}

	return(0);
}
